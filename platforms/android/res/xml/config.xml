<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Matafuegos Ya</name>
    <description>
        La App que mas sabe de incendios!
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Widux.com.ar
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon qualifier="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon qualifier="mdpi" src="res/icon/android/icon-48-mdpi.png" />
    <icon qualifier="hdpi" src="res/icon/android/icon-72-hdpi.png" />
    <icon qualifier="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    <splash qualifier="land-ldpi" src="res/screen/android/screen-hdpi-landscape.png" />
    <splash qualifier="land-mdpi" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash qualifier="land-hdpi" src="res/screen/android/screen-ldpi-landscape.png" />
    <splash qualifier="land-xhdpi" src="res/screen/android/screen-ldpi-portrait.png" />
    <splash qualifier="land-xxhdpi" src="res/screen/android/screen-mdpi-landscape.png" />
    <splash qualifier="land-xxxhdpi" src="res/screen/android/screen-mdpi-portrait.png" />
    <splash qualifier="port-ldpi" src="res/screen/android/screen-xhdpi-landscape.png" />
    <splash qualifier="port-mdpi" src="res/screen/android/screen-xhdpi-portrait.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
